#include<stdio.h>
#include<conio.h>
int  main()
{
    int factorial(int);
    int num,fact;
    printf("Enter the number\n");
    scanf("%d",&num);
    fact=factorial(num);
    printf("The fact of %d is %d",num,fact);

}
int factorial(int num)
{
    int i,fact=1;
    for(i=1;i<=num;i++)
    {
        fact=fact*i;
    }
    return(fact);
}


OUTPUT:
Enter the number
10
The fact of 10 is 3628800
