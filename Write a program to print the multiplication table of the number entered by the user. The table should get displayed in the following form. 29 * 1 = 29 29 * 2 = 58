#include<stdio.h>
void main()
{
   int i,num,table;
   printf("Enter number?  ");
   scanf("%d",&num);
   printf("\t\tMULTIPLICATION TABLE\n");

   for(i=1;i<=num;i++)
   {
       table=29*i;
       printf("\t\t    29 * %d = %d\n",i,table);

   }
   
   output:
   Enter number?  10
                MULTIPLICATION TABLE
                    29 * 1 = 29
                    29 * 2 = 58
                    29 * 3 = 87
                    29 * 4 = 116
                    29 * 5 = 145
                    29 * 6 = 174
                    29 * 7 = 203
                    29 * 8 = 232
                    29 * 9 = 261
                    29 * 10 = 290
