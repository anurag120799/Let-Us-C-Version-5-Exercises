#include<stdio.h>
#include<conio.h>
void main()
{
    int num,a,b,c,d,e,d5,d4,d3,d2,d1;
    printf("Enter the 5 digits\n");
    scanf("%d",&num);

    a=num/10;
    d5=num%10;

    b=a/10;
    d4=a%10;
    
    c=b/10;
    d3=b%10;
    
    d=c/10;
    d2=c%10;
    
    e=d/10;
    d1=d%10;

     printf("The sum of the entered digits %d is %d%d%d%d%d",num,d5,d4,d3,d2,d1);
}
