#include<stdio.h>
#include<conio.h>
int main()
{
    int a,b,power,i=1;
    printf("Enter base value a  ");
    scanf("%d",&a);
    printf("Enter base value b  ");
    scanf("%d",&b);

    for(power=1;i<=b;i++)
    {
        power=power*a;
    }
    printf("%d",power);
}

output:
Enter base value a  2
Enter base value b  2
2 power 2 is 4
